class Solution {
    public boolean canPlaceFlowers(int[] fbed, int n) {
        int c =0;
        for(int i=0; i< fbed.length; i++){
            if(fbed[i]==0){
                c+=1;
            }
        }
        if(c%n==0){
            return true;
        }
        else {
            return false;
        }
    }
}
